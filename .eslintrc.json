{
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": "off",
        "react/prop-types": "off",
        "react/jsx-props-no-spreading": "off",
        "react/no-unstable-nested-components": "off",
        "no-useless-return": "off",
        "no-param-reassign": "off"
    }
}